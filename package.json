{
  "name": "msw-devtool",
  "private": true,
  "packageManager": "pnpm@10.6.5",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "build": "pnpm --filter ./packages/core run build",
    "dev": "pnpm --filter ./packages/core run dev",
    "dev:demo": "pnpm --filter demo run dev",
    "typecheck": "pnpm -r run typecheck",
    "clean": "pnpm -r run clean",
    "test": "vitest",
    "test:ci": "vitest run",
    "lint": "biome check .",
    "test:e2e": "playwright test",
    "format": "biome check --write ."
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.4",
    "@playwright/test": "^1.58.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "jsdom": "^28.1.0",
    "tsup": "^8.4.0",
    "typescript": "^5.7.0",
    "ultracite": "^7.2.3",
    "vitest": "^4.0.18"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "msw"
    ]
  },
  "msw": {
    "workerDirectory": [
      "apps/demo/public"
    ]
  }
}